-------------------------------
scanf("%d", s);
int temp[];
int temp_top = 0;
for (int i = 0; i < top; i++) {
	if (stack[i] != s) {
		temp[temp_top++] = stack[i];
		stack[i] = \0;
	}
}
top = 0;
for (int i = 0; i < temp_top; i++ {
	stack[top++] = temp[i];
}
